  <profile>
    <id>webapi-bigquery</id>
    <properties>             
      <datasource.driverClassName>net.starschema.clouddb.jdbc.BQDriver</datasource.driverClassName>
      <datasource.url>jdbc:BQDriver:ohdsi-in-a-box/ohdsi?withServiceAccount=true&amp;transformQuery=false</datasource.url>
